{
    "name": "simpleminds/laravel-connections",
    "description": "This package gives Eloquent models the ability to manage their connections.",
    "keywords": ["laravel", "connections", "connection-system", "connections", "eloquent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Simpleminds",
            "email": "dev@simpleminds.in"
        }
    ],
    "autoload-dev": {
        "classmap": [
            "vendor/laravel/laravel/tests/TestCase.php"
        ],
        "files": [
            "tests/helpers.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "7.*",
        "fzaninotto/faker": "^1.4",
        "laravel/laravel": "6.*",
        "codeclimate/php-test-reporter": "^0.3.2",
        "mockery/mockery": "^0.9.5",
        "doctrine/dbal": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Simpleminds\\Connections\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Simpleminds\\Connections\\ConnectionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
